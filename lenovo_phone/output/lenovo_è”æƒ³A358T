<script>
$(document).ready(function(){
	cnmoProductPkObj.setDefault({
		"del_callback" : function (id) {
            if ($("#param_"+id).attr("rel")) {
                $("#joinPkByParam").removeClass("contrast-c").addClass("contrast-b");
            }
		},
		"clearall_callback" : function () {
			$("#joinPkByParam").removeClass("contrast-c").addClass("contrast-b");
		},
		'rechecked_callback' : function (id) {
			if ($("#param_"+id).attr("rel")) {
                $("#joinPkByParam").removeClass("contrast-b").addClass("contrast-c");
            }
		}
	});
	
	cnmoProductPkObj.rechecked();

	$('#joinPkByParam').click(function(){
		if ($('#joinPkByParam').attr("class") == "contrast-b") {
			var proRow = $(".product-int").attr("rel").split("|");
	        cnmoProductPkObj.add(proRow[0], proRow[1], proRow[2], proRow[3], proRow[4], function (info) {
	            if (info.error == 0) {
	                $('#joinPkByParam').removeClass("contrast-b").addClass("contrast-c");
	            } else {
	                alert(info.msg);
	            }
	        });
		} else {
			cnmoProductPkObj.del($(".product-int").attr('id').replace('param_', ''));
		}
    });
});
</script> <!--<div class="mt30"><a href="#"><img src="images/ad.jpg" height="200" width="220" /></a></div> -->
