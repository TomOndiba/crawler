            $('#newestProductsRank .fenlei-title li').mouseover(function(){
                $(this).siblings().removeClass('cur');
                $(this).addClass('cur');
                var liIndex = $(this).index();
                $('#newestProductsRank').children('ul').hide();
                $('#newestProductsRank').children('ul').eq(liIndex).show();
            });
            $('#newestProductsRank > ul').children('li').mouseover(function(){
                $(this).siblings().children('dl').hide();
                $(this).children('dl').show();
            });
