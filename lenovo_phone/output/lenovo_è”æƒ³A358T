<script type="text/javascript">
	/*鼠标移入添加className 移出删除className*/
	function TabClassName(obj,className){
	    obj.live('mouseenter', function() {
	        $(this).addClass(className);
	}).live('mouseleave',function() {
	        $(this).removeClass(className);
	    });
	}
	
	var addCurs = new TabClassName($('.PROweb-layer-dl, .PROweixin, .PROapp'),'cur');
	var addCurs = new TabClassName($('.PROloginon, .PROloginon'),'PROcur');

	/*网站地图js单写，有点冲突。鼠标移入添加样式cur，移出删除cur样式*/
	$(".PROweb-nav").hover(function(){
	    $(this).addClass('cur');
	},function(){
	    $(this).removeClass('cur');
	});
	/*显示微信*/
    $('.PROwxdiv').click(function(){
        var $wxma = $(this).next('.wxma');
        lazy_src($wxma.find('img[lazy_src]'));
        $wxma.show();
    });
    $('.PROweixin').mouseleave(function(){
        $(this).find('.wxma').hide();
    });
    
    var PROwx=document.getElementById('PROwx');
    var PROwxdiv=document.getElementById('PROwxdiv');
    var PROwxma =document.getElementById('PROwxma');
    PROwxdiv.onclick=function(){
        PROwxma.style.display='block';
    };
    PROwx.onmouseleave=function(){
        PROwxma.style.display='none';
}
</script><!--顶部 结束-->
